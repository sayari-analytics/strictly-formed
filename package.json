{
  "name": "strictly-formed",
  "version": "0.2.0",
  "description": "statically typed forms for typescript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json --format stylish",
    "test": "node test/",
    "validate": "npm run lint && npm run test",
    "deploy:patch": "npm run validate && npm version patch && npm run build && npm publish",
    "deploy:minor": "npm run validate && npm version minor && npm run build && npm publish",
    "deploy:major": "npm run validate && npm version major && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sayari-analytics/strictly-formed.git"
  },
  "keywords": [
    "typescript",
    "react",
    "form"
  ],
  "peerDependencies": {
    "react": "16.x"
  },
  "author": "James Conkling <james.lane.conkling@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sayari-analytics/strictly-formed/issues"
  },
  "homepage": "https://github.com/sayari-analytics/strictly-formed#readme",
  "devDependencies": {
    "@types/react": "^16.4.9",
    "react": "^16.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  }
}
